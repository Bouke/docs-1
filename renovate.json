{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^build-process.yml$"
      ],
      "matchStrings": [
        "NUNIT_VERSION_FOR_API_DOCS: \"(?<currentValue>.*?)\""
      ],
      "depNameTemplate": "docfx",
      "datasourceTemplate": "nuget"
    }
  ]
}